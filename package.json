    "dayjs": "^1.11.13",
    "geist": "^1.3.1",
    "next": "^15.1.6",
    "next-auth": "5.0.0-beta.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "sonner": "^1.7.4",
